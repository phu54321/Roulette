function onPluginStart() {
    MPQAddFile(
        'staredit\\wav\\onSelect.ogg',
        py_open('res\\onSelect.ogg', 'rb').read()
    );

    MPQAddFile(
        'staredit\\wav\\onRoll.ogg',
        py_open('res\\onRoll.ogg', 'rb').read()
    );

    RunAIScript("Turn ON Shared Vision for Player 8");
    SetMemory(0x51CE98, SetTo, 2);
}
